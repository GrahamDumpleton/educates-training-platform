#!/bin/bash

if [ ! -f /opt/eduk8s/download-url.txt ]; then
  echo "Error: Workshop has not previously been downloaded." >&2
  exit 1
fi

download-workshop `cat /opt/eduk8s/download-url.txt`

for script in /opt/eduk8s/workshop/setup.d/*.sh; do
    if [ -x "$script" ]; then
        $script
    fi
done
